{
    "log": {
        "access": "",
        "error": "",
        "loglevel": "warning"
    },
    "dns": {
        "servers": [
            "1.1.1.1"
        ],
        "queryStrategy": "UseIPv4"
    },
    "routing": {
        "domainStrategy": "IPIfNonMatch",
        "rules": [
            {
                "type": "field",
                "ip": [
                    "1.1.1.1"
                ],
                "outboundTag": "direct"
            },
            {
                "type": "field",
                "domain": [
                    "geosite:category-ads-all",
                    "geosite:cn"
                ],
                "outboundTag": "block"
            },
            {
                "type": "field",
                "ip": [
                    "geoip:cn",
                    "geoip:private"
                ],
                "outboundTag": "block"
            }
        ]
    },
    "inbounds": [
        {
            "listen": "0.0.0.0",
            "port": 443, //端口
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "id": "2388264d-3312-470a-a883-fdd60deed880", //用户ID
                        "flow": "xtls-rprx-direct"
                    }
                ],
                "decryption": "none",
                "fallbacks": [
                    {
                        "alpn": "",
                        "path": "",
                        "dest": "8001",
                        "xver": 2
                    },
                    {
                        "alpn": "h2",
                        "path": "",
                        "dest": "8002",
                        "xver": 2
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "xtls",
                "xtlsSettings": {
                    "minVersion": "1.3",
                    "cipherSuites": "TLS_AES_128_GCM_SHA256:TLS_AES_256_GCM_SHA384:TLS_CHACHA20_POLY1305_SHA256:TLS_ECDHE_ECDSA_WITH_AES_128_GCM_SHA256:TLS_ECDHE_ECDSA_WITH_AES_256_GCM_SHA384:TLS_ECDHE_ECDSA_WITH_CHACHA20_POLY1305_SHA256",
                    "alpn": [
                        "h2",
                        "http/1.1"
                    ],
                    "certificates": [
                        {
                            "certificateFile": "/etc/ssl/private/fullchain.pem", //SSL证书公钥
                            "keyFile": "/etc/ssl/private/privkey.pem", //SSL证书私钥
                            "ocspStapling": 3600
                        }
                    ],
                    "rejectUnknownSni": true
                }
            }
        },
        {
          "port": 45914,
          "protocol": "vmess",
          "settings": {
            "clients": [
              {
                "id": "955284fa-f9dd-4bd1-b02f-776057c1000d",
                "alterId": 0
              }
            ]
          },
          "streamSettings": {
            "network": "tcp",
            "security": "none",
            "tcpSettings": {
              "header": {
                "type": "http",
                "request": {
                  "method": "GET",
                  "path": [
                    "/"
                  ],
                  "headers": {}
                },
                "response": {
                  "version": "1.1",
                  "status": "200",
                  "reason": "OK",
                  "headers": {}
                }
              }
            }
          }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "settings": {
                "domainStrategy": "UseIPv4"
            },
            "tag": "direct"
        },
        {
            "protocol": "blackhole",
            "settings": {
                "response": {
                    "type": "http"
                }
            },
            "tag": "block"
        }
    ]
}
